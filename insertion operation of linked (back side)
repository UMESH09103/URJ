//for practice
#include <iostream>
using namespace std;
class node
{
public:
    int data;
    node *next;
    node(int value)
    {
        data = value;
        next = NULL;
    }
};
int main()
{
    node *head;
    head = NULL;
    node *tail;
    tail = NULL;
    int arr[] = {3, 4, 5, 33,  55};
    for (int i = 0; i < 5; i++)
    {
        if (head == NULL)
        {
            head = new node(arr[i]);
            tail=head;
        }
        else{
            tail->next=new node(arr[i]);
            tail=tail->next;

        }
    }
    node *temp;
    temp=head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        cout<<temp->next<<" ";
        temp=temp->next;

    }
    return 0;
}
